   "source": [
    "import numpy as np\n",
    "from scipy import stats as sp\n",
    "#카이제곱 검정\n",
    "N = 10\n",
    "K = 4\n",
    "mu_0 = np.ones(K)/K\n",
    "np.random.seed(0)\n",
    "x = np.random.choice(K, N, p=mu_0)\n",
    "n = np.bincount(x, minlength=K)\n",
    "sp.stats.chisquare(n)\n"
   ]
